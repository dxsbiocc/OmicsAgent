{
  "name": "独立坐标折线图",
  "description": "独立坐标折线图将两个数据集绘制为两个不同的坐标轴，用于展示两个数据集之间的关系。",
  "params_schema": {
    "data": {
      "type": "string",
      "description": "CSV file path"
    },
    "x_col": {
      "type": "string"
    },
    "y_col": {
      "type": "string"
    },
    "color_col": {
      "type": [
        "string",
        "null"
      ]
    },
    "point_size": {
      "type": "number"
    },
    "width": {
      "type": "integer"
    },
    "height": {
      "type": "integer"
    },
    "title": {
      "type": "string"
    }
  },
  "defaults": {
    "x_col": "x",
    "y_col": "y",
    "color_col": null,
    "point_size": 2.0,
    "width": 800,
    "height": 600,
    "title": "独立坐标折线图"
  },
  "has_js": true
}