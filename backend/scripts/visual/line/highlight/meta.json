        "null"
      ]
    },
    "point_size": {
      "type": "number"
    },
    "width": {
      "type": "integer"
    },
    "height": {
      "type": "integer"
    },
    "title": {
      "type": "string"
    }
  },
  "defaults": {
    "x_col": "x",
    "y_col": "y",
