{
  "name": "哑铃图",
  "description": "哑铃图是一种特殊的散点图，适合展示多个类别的数值",
  "ggplot2": {
    "mapping": {
      "x": "age_group",
      "y": "mean_prev",
      "colour": "Sex"
    },
    "layers": [
      {
        "type": "geom_line",
        "mapping": {
          "group": "age_group"
        },
        "arguments": {
          "colour": "#cba7a2"
        }
      },
      {
        "type": "geom_point",
        "arguments": {
          "size": 5,
          "shape": 21,
          "fill": "#ffffff"
        }
      },
      {
        "type": "geom_point",
        "arguments": {
          "size": 3
        }
      }
    ],
    "labs": {
      "type": "labs",
      "arguments": {
        "title": "",
        "x": "",
        "y": "Mean Prevalence of obesity"
      }
    },
    "scales": [
      {
        "type": "scale_colour_manual",
        "arguments": {
          "values": [
            "#f784b6",
            "#79ceb8"
          ]
        }
      }
    ],
    "guides": {
      "x": {
        "type": "guide_prism_bracket"
      }
    },
    "themes": [
      {
        "type": "theme_prism"
      },
      {
        "type": "theme",
        "arguments": {
          "axis.text.x": {
            "type": "element_text",
            "arguments": {
              "angle": 90
            }
          },
          "plot.background": {
            "type": "element_blank"
          }
        }
      }
    ],
    "width": 800,
    "height": 600
  },
  "has_js": false
}