{
  "name": "分组散点图",
  "description": "绘制分组散点图.",
  "ggplot2": {
    "mapping": {
      "x": "x",
      "y": "y",
      "fill": "cluster"
    },
    "layers": [
      {
        "type": "geom_vline",
        "arguments": {
          "xintercept": 0,
          "linewidth": 0.2
        }
      },
      {
        "type": "geom_hline",
        "arguments": {
          "yintercept": 0,
          "linewidth": 0.2
        }
      },
      {
        "type": "geom_point",
        "arguments": {
          "shape": 21,
          "size": 4,
          "alpha": 0.8
        }
      }
    ],
    "labs": {
      "type": "labs",
      "arguments": {
        "title": "Scatter Plot of Clusters",
        "x": "pc1",
        "y": "pc2"
      }
    },
    "scales": [
      {
        "type": "scale_fill_manual",
        "arguments": {
          "values": [
            "#1CC7D0",
            "#2DDE98",
            "#FFC168"
          ]
        }
      }
    ],
    "themes": [
      {
        "type": "theme_minimal"
      }
    ],
    "width": 800,
    "height": 600
  },
  "has_js": true
}