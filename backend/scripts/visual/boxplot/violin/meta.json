{
  "name": "小提琴图",
  "description": "小提琴图用于展示变量在不同分组中的分布情况",
  "ggplot2": {
    "mapping": {
      "x": "gene",
      "y": "expression"
    },
    "layers": [
      {
        "type": "geom_violin"
      },
      {
        "type": "geom_round_boxplot",
        "mapping": {
          "fill": "gene"
        },
        "arguments": {
          "radius": 2,
          "width": 0.2,
          "errorbar.draw": false,
          "show.legend": false
        }
      }
    ],
    "scales": [
      {
        "type": "scale_fill_manual",
        "arguments": {
          "name": null,
          "values": [
            "#009392",
            "#39b185",
            "#9ccb86",
            "#e9e29c",
            "#eeb479",
            "#e88471",
            "#cf597e"
          ]
        }
      }
    ],
    "guides": {
      "x": {
        "type": "guide_prism_bracket"
      }
    },
    "labs": {
      "type": "labs",
      "arguments": {
        "title": "",
        "x": "",
        "y": "Gene expression level"
      }
    },
    "themes": [
      {
        "type": "theme_prism"
      },
      {
        "type": "theme",
        "arguments": {
          "plot.background": {
            "type": "element_blank"
          }
        }
      }
    ],
    "width": 600,
    "height": 500
  },
  "has_js": false,
  "has_r": true
}