{
  "name": "差异表达云雨图",
  "description": "差异表达云雨图用于展示变量在不同分组中的分布情况",
  "ggplot2": {
    "mapping": {
      "x": "gender",
      "y": "TP53"
    },
    "layers": [
      {
        "type": "geom_half_violin",
        "mapping": {
          "x": 0.05,
          "fill": "gender"
        },
        "arguments": {
          "colour": "grey50"
        }
      },
      {
        "type": "geom_jitter",
        "arguments": {
          "width": 0.03
        }
      },
      {
        "type": "geom_half_round_boxplot",
        "mapping": {
          "x": 1.05
        },
        "arguments": {
          "side": "right",
          "width": 0.2
        }
      },
      {
        "type": "geom_half_round_boxplot",
        "mapping": {
          "x": 1.95
        },
        "arguments": {
          "side": "left",
          "width": 0.2
        }
      },
      {
        "type": "geom_line",
        "mapping": {
          "x": "x",
          "y": "q",
          "group": "prob",
          "colour": "signif"
        }
      },
      {
        "type": "geom_point",
        "mapping": {
          "x": "x",
          "y": "q",
          "colour": "signif"
        },
        "arguments": {
          "size": 4
        }
      },
      {
        "type": "annotate",
        "arguments": {
          "geom": "text",
          "x": [
            1.3,
            1.6
          ],
          "y": [
            4.3
          ]
        }
      }
    ],
    "scales": [
      {
        "type": "scale_fill_manual",
        "arguments": {
          "values": [
            "#a0c443",
            "#5da4dc"
          ]
        }
      },
      {
        "type": "scale_colour_manual",
        "arguments": {
          "values": [
            "grey80",
            "black"
          ]
        }
      },
      {
        "type": "scale_x_continuous",
        "arguments": {
          "breaks": [
            1,
            2
          ]
        }
      }
    ],
    "coordinates": {
      "type": "coord_flip"
    },
    "labs": {
      "type": "labs",
      "arguments": {
        "title": "",
        "x": "Group",
        "y": "Gene expression level (TP53)"
      }
    },
    "themes": [
      {
        "type": "theme_prism"
      },
      {
        "type": "theme",
        "arguments": {
          "axis.text.y": {
            "type": "element_blank"
          },
          "axis.ticks.y": {
            "type": "element_blank"
          },
          "panel.background": {
            "type": "element_blank"
          }
        }
      }
    ],
    "width": 800,
    "height": 500
  },
  "has_js": false,
  "has_r": true
}