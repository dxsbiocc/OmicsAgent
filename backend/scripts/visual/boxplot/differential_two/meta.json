{
  "name": "双组差异表达箱线图",
  "description": "双组差异表达箱线图用于变量在两个分组之间的差异表达情况",
  "ggplot2": {
    "mapping": {
      "x": "gene",
      "y": "exprs",
      "fill": "group"
    },
    "layers": [
      {
        "type": "geom_round_boxplot",
        "arguments": {
          "outliers": false
        }
      },
      {
        "type": "stat_compare_means",
        "mapping": {
          "group": "group"
        },
        "arguments": {
          "label": "p.signif",
          "hide.ns": true
        }
      }
    ],
    "scales": [
      {
        "type": "scale_fill_manual",
        "arguments": {
          "values": [
            "#d7689d",
            "#f3d337",
            "#a0c443",
            "#66b5ae",
            "#5da4dc",
            "#40769e"
          ]
        }
      }
    ],
    "guides": {
      "x": {
        "type": "guide_prism_bracket"
      }
    },
    "labs": {
      "type": "labs",
      "arguments": {
        "title": "",
        "x": "",
        "y": "Gene expression level"
      }
    },
    "themes": [
      {
        "type": "theme_prism"
      },
      {
        "type": "theme",
        "arguments": {
          "plot.background": {
            "type": "element_blank"
          },
          "plot.title": {
            "type": "element_markdown",
            "arguments": {
              "size": 14,
              "hjust": 0.5
            }
          }
        }
      }
    ],
    "width": 800,
    "height": 600
  },
  "has_js": false,
  "has_r": true
}